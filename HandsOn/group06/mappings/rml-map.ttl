  ];

  rr:subjectMap [
    rr:template "http://accidentestrafico.org/traffic/resource/Coordenadas/{idCoordenadas}";
    rr:class :Coordenadas
  ];

  rr:predicateObjectMap [
    rr:predicate :coordenadax;
